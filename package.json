{
  "name": "art-map-generator",
  "version": "1.0.1",
  "description": "A solution to generate MapArt for Minecraft",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "author": "Half-Shield",
  "license": "ISC",
  "dependencies": {
    "jquery": "^3.6.0",
    "mineflayer": "^3.11.2",
    "mineflayer-pathfinder": "^1.8.0",
    "mineflayer-web-inventory": "^1.6.3",
    "uaup-js": "^0.0.22"
  },
  "devDependencies": {
    "electron": "^12.0.0",
    "electron-builder": "^22.13.1"
  },
  "build": {
    "appId": "com.halfshield.artmapgenerator",
    "productName": "ArtMap Generator",
    "target": "NSIS",
    "directories": {
      "output": "../realse"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true
    }
  }
}
